{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["scripts/*.{ts,js}", "app/server/*.ts", "app/library/icon/Icon.tsx"],
	"remix": true,
	"lefthook": true,
	"project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,css}"],
	"ignore": [
		"app/library/icon/icons/types.ts",
		"react-router.config.ts",
		"app/components/ui/**",
		"app/components/typography/**",
		"app/tailwind.css"
	],
	"ignoreDependencies": ["@babel/preset-typescript", "babel-plugin-react-compiler", "tailwindcss", "tw-animate-css"]
}
